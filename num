import java.util.Scanner;

public class numb {

	public static void main(String[] args) {
		for(;;){
		Scanner Read=new Scanner(System.in);
		
		int num=Read.nextInt();
		if(num>0)
			System.out.println("positive");
		else if(num<0)
			System.out.println("negative");
		else 
			System.out.println("zero");
		}

	}

}
